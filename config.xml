<?xml version='1.0' encoding='utf-8'?>
<widget id="com.neptuno.eta" version="1.0.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ETAFASHION</name>
    <description>
        Aplicación etafashion
    </description>
    <author email="andres@neptuno.ec" href="https://www.facebook.com/neptuno.ec">
        Andrés López
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="img/icons/icon.png" />
    <universal-links>
        <host name="https://etafashionapp.page.link" scheme="https" />
        <host name="https://etafashion-pushnotifications.firebaseio.com" scheme="https">
            <path url="/__/auth/callback" />
        </host>
    </universal-links>
    <preference name="AndroidLaunchMode" value="singleTask" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^7.1.1" />
    <plugin name="cordova-plugin-geolocation" spec="^2.1.0" />
    <plugin name="cordova-plugin-buildinfo" spec="^2.0.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
    <plugin name="cordova-plugin-customurlscheme" spec="^4.3.0">
        <variable name="URL_SCHEME" value="com.firebase.cordova" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-browsersync" spec="^1.1.0" />
    <plugin name="cordova-plugin-firebase" spec="^1.0.5" />
    <plugin name="cordova-universal-links-plugin" spec="git+https://github.com/walteram/cordova-universal-links-plugin.git" />
    <plugin name="cordova-plugin-browsertab" spec="^0.2.0" />
    <plugin name="cordova-plugin-facebook4" spec="^2.3.0">
        <variable name="APP_ID" value="1421229451228873" />
        <variable name="APP_NAME" value="Etafashion" />
        <variable name="ANDROID_SDK_VERSION" value="4.+" />
    </plugin>
</widget>
